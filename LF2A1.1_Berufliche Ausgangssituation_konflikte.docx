<script>
$(document).ready(function() {
    if (window.location.href.indexOf("https://lms.lernen.hamburg/login/index.php") > -1) {
        // Adding the eye icon and styling
        var formGroup = $('div.form-group:eq(1)');
        var passwordInput = formGroup.find('input#password');

        // Create a container to hold the input field and the eye icon
        var containerDiv = $('<div class="input-group"></div>');

        // Append the password input field to the container
        containerDiv.append(passwordInput);

        // Append the eye icon to the container
        var eyeIconDiv = $('<div class="input-group-append" style="cursor: pointer;"><span class="input-group-text"><i class="fa fa-eye-slash" id="eyeIcon" onclick="togglePasswordVisibility()"></i></span></div>');
        containerDiv.append(eyeIconDiv);

        // Replace the formGroup content with the new container
        formGroup.html(containerDiv);

        // Styling the password input field
        passwordInput.addClass("form-control form-control-lg");
        passwordInput.css("color", "#000");
        passwordInput.css("border", "1px solid #8f959e");
    }
});

function togglePasswordVisibility() {
    var passwordInput = $('div.form-group:eq(1) input#password');
    var eyeIcon = $('#eyeIcon');

    if (passwordInput.attr('type') === 'password') {
        // Show password
        passwordInput.attr("type", "text");
        eyeIcon.removeClass('fa-eye-slash');
        eyeIcon.addClass('fa-eye');
    } else {
        // Hide password
        passwordInput.attr("type", "password");
        eyeIcon.removeClass('fa-eye');
        eyeIcon.addClass('fa-eye-slash');
    }
}
